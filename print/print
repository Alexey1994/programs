Screen screen
write  write_in_screen

init() {
	screen.init(0xB8000)
	write_in_screen.init(screen, screen.write_bytes)
}

_Number(Number number) {
	number_copy = number
	right_selector = 1

	cycle {
		number_copy = number_copy / 10
		!number_copy => break
		right_selector = right_selector * 10
	}

	cycle {
		write_in_screen._Byte(number / right_selector % 10 + '0')
		right_selector = right_selector / 10
		!right_selector => break
	}
}


#Number a
#print(a)
#print(1)